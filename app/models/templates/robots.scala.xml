@(userAnswers:utils.UserAnswers,robotsXmlHelper:utils.RobotsXmlHelper)
<!--<?xml version="1.0" encoding="UTF-8" standalone="no"?>-->
<ctr>
    <userDetails>
        <name>@userAnswers.userDetails.get.name</name>
        <nino>@userAnswers.userDetails.get.nino</nino>
    </userDetails>
    <claimSection>
        <selectedTaxYear>@robotsXmlHelper.getSelectTaxYear</selectedTaxYear>
        @if(userAnswers.employmentDetails.isDefined){<employmentDetails>@userAnswers.employmentDetails</employmentDetails>}
        @if(userAnswers.enterPayeReference.isDefined){<payeReference>@userAnswers.enterPayeReference</payeReference>}
        @if(userAnswers.detailsOfEmploymentOrPension.isDefined){<detailsOfEmploymentOrPension>@userAnswers.detailsOfEmploymentOrPension</detailsOfEmploymentOrPension>}
    </claimSection>
    @if(userAnswers.anyBenefits){
    <benefitSection>
        <anyBenefits>@userAnswers.anyBenefits</anyBenefits>
        <selectBenefits>@userAnswers.selectBenefits</selectBenefits>
        @if(userAnswers.howMuchBereavementAllowance.isDefined){<howMuchBereavementAllowance>
        @userAnswers.howMuchBereavementAllowance</howMuchBereavementAllowance>}
        @if(userAnswers.howMuchCarersAllowance.isDefined){<howMuchCarersAllowance>
        @userAnswers.howMuchCarersAllowance</howMuchCarersAllowance>}
        @if(userAnswers.howMuchJobseekersAllowance.isDefined){<howMuchJobseekersAllowance>
        @userAnswers.howMuchJobseekersAllowance</howMuchJobseekersAllowance>}
        @if(userAnswers.howMuchEmploymentAndSupportAllowance.isDefined){<howMuchEmploymentAndSupportAllowance>
        @userAnswers.howMuchEmploymentAndSupportAllowance</howMuchEmploymentAndSupportAllowance>}
        @if(userAnswers.howMuchIncapacityBenefit.isDefined){<howMuchIncapacityBenefit>
        @userAnswers.howMuchIncapacityBenefit</howMuchIncapacityBenefit>}
        @if(userAnswers.howMuchStatePension.isDefined){<howMuchStatePension>
        @userAnswers.howMuchStatePension</howMuchStatePension>}
        <otherBenefitsSection>
            @robotsXmlHelper.getOtherBenefits(userAnswers.otherBenefit.get)
        </otherBenefitsSection>
    </benefitSection>
    }
    @if(userAnswers.anyCompanyBenefits){
    <companyBenefitsSection>
        <anyCompanyBenefits>@userAnswers.anyCompanyBenefits</anyCompanyBenefits>
        <selectCompanyBenefits>@userAnswers.selectCompanyBenefits</selectCompanyBenefits>
        @if(userAnswers.howMuchCarBenefits.isDefined){<howMuchCarBenefits>
        @userAnswers.howMuchCarBenefits</howMuchCarBenefits>}
        @if(userAnswers.howMuchFuelBenefit.isDefined){<howMuchFuelBenefit>
        @userAnswers.howMuchFuelBenefit</howMuchFuelBenefit>}
        @if(userAnswers.howMuchMedicalBenefits.isDefined){<howMuchMedicalBenefits>
        @userAnswers.howMuchMedicalBenefits</howMuchMedicalBenefits>}
        <otherCompanyBenefitsSection>
            @robotsXmlHelper.getOtherCompanyBenefits(userAnswers.otherCompanyBenefit.get)
        </otherCompanyBenefitsSection>
    </companyBenefitsSection>
    }
    @if(userAnswers.anyTaxableIncome){
    <taxableIncomeSection>
        <anyTaxableIncome>@userAnswers.anyTaxableIncome</anyTaxableIncome>
        <selectTaxableIncome>@userAnswers.selectTaxableIncome</selectTaxableIncome>
        @if(userAnswers.howMuchRentalIncome.isDefined){
        <rentalIncome>
            <howMuch>@userAnswers.howMuchRentalIncome</howMuch>
            <anyTaxPaid>@robotsXmlHelper.getAnyIncome(userAnswers.anyTaxableRentalIncome.get)</anyTaxPaid>
            <taxPaid>@robotsXmlHelper.getIncomeAmount(userAnswers.anyTaxableRentalIncome.get)</taxPaid>
        </rentalIncome>
        }
        @if(userAnswers.howMuchBankInterest.isDefined){
        <bankInterest>
            <howMuch>@userAnswers.howMuchBankInterest</howMuch>
            <anyTaxPaid>@robotsXmlHelper.getAnyIncome(userAnswers.anyTaxableBankInterest.get)</anyTaxPaid>
            <taxPaid>@robotsXmlHelper.getIncomeAmount(userAnswers.anyTaxableBankInterest.get)</taxPaid>
        </bankInterest>
        }
        @if(userAnswers.howMuchInvestmentOrDividend.isDefined){
        <investmentOrDividend>
            <howMuch>@userAnswers.howMuchInvestmentOrDividend</howMuch>
            <anyTaxPaid>@robotsXmlHelper.getAnyIncome(userAnswers.anyTaxableInvestments.get)</anyTaxPaid>
            <taxPaid>@robotsXmlHelper.getIncomeAmount(userAnswers.anyTaxableInvestments.get)</taxPaid>
        </investmentOrDividend>
        }
        @if(userAnswers.howMuchForeignIncome.isDefined){
        <foreignIncome>
            <howMuch>@userAnswers.howMuchForeignIncome</howMuch>
            <anyTaxPaid>@robotsXmlHelper.getAnyIncome(userAnswers.anyTaxableForeignIncome.get)</anyTaxPaid>
            <taxPaid>@robotsXmlHelper.getIncomeAmount(userAnswers.anyTaxableForeignIncome.get)</taxPaid>
        </foreignIncome>
        }
        @robotsXmlHelper.getOtherTaxableIncome(userAnswers.otherTaxableIncome.get)
    </taxableIncomeSection>
    }
    <paymentSection>
        <whereToSendThePayment>@userAnswers.whereToSendPayment</whereToSendThePayment>
        @if(userAnswers.paymentAddressCorrect.isDefined){<paymentAddressCorrect>@userAnswers.paymentAddressCorrect</paymentAddressCorrect>}
        @if(userAnswers.nomineeFullName){<nomineeFullname>@userAnswers.nomineeFullName</nomineeFullname>}
        @if(robotsXmlHelper.getAnyAgentRef(userAnswers.anyAgentRef.get)){<anyAgentRef>@robotsXmlHelper.getAnyAgentRef(userAnswers.anyAgentRef.get)</anyAgentRef>}
        @if(robotsXmlHelper.getAnyAgentReference(userAnswers.anyAgentRef.get)){<agentReference>@robotsXmlHelper.getAnyAgentReference(userAnswers.anyAgentRef.get)</agentReference>}
        @if(userAnswers.isPaymentAddressInTheUK){<isPaymentAddressInTheUK>@userAnswers.isPaymentAddressInTheUK</isPaymentAddressInTheUK>}
        <paymentAddress>
        @if(userAnswers.paymentUKAddress){@robotsXmlHelper.getUkAddress}
        @if(userAnswers.paymentInternationalAddress){@robotsXmlHelper.getInternationalAddress}
        @if(userAnswers.paymentLookupAddress){@userAnswers.paymentLookupAddress}
        </paymentAddress>
    </paymentSection>
    <contactSection>
        <anyTelephoneNumber>@robotsXmlHelper.getAnyTelephone(userAnswers.anyTelephoneNumber.get)</anyTelephoneNumber>
        <telephoneNumber>@robotsXmlHelper.getAnyTelephoneNumber(userAnswers.anyTelephoneNumber.get)</telephoneNumber>
    </contactSection>
</ctr>